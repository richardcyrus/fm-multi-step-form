{
  "name": "fm-multi-step-form",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier \"**/*.(c|m)[jt]s?(x)\"",
    "check": "prettier --write \"**/*.(c|m)[jt]s?(x)\" && eslint --fix \"**/*.[jt]s?(x)\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-devtools": "^0.9.0",
    "@tanstack/react-router": "^1.144.0",
    "@tanstack/react-router-devtools": "^1.144.0",
    "@tanstack/react-router-ssr-query": "^1.144.0",
    "@tanstack/react-start": "^1.145.3",
    "@tanstack/router-plugin": "^1.145.2",
    "nitro": "latest",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@tanstack/devtools-vite": "^0.4.0",
    "@tanstack/eslint-config": "^0.3.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "stylelint": "^16.26.1",
    "stylelint-config-clean-order": "^8.0.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "web-vitals": "^5.1.0"
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix"
    ],
    "*.{js,jsx,cjs,mjs,ts,tsx,mts}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
